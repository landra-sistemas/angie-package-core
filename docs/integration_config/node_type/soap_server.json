    "data": {
        "id": "a91d0926-6ca8-4d41-930e-d99c3cde9b72",
        "code": "soap_server",
        "name": "SOAP Server",
        "group": "Comunicaciones",
        "name_": "SOAP Client CLONED",
        "handles": "in",
        "defaults": "{\n    \"protocol\": \"http\",\n    \"cxf_query_params\": {\n        \"dataFormat\": \"RAW\"\n    }\n}",
        "package_code": "angie.core",
        "xml_template": "<route id=\"{{source}}\">\n    {{#if (ne soap_action null)}}\n    <setHeader name=\"SOAPAction\">\n\t\t<constant>{{soap_action}}</constant>\n\t</setHeader>\n\t{{/if}}\n    <from uri=\"cxf://{{protocol}}://{{hostname}}:{{port}}{{path}}{{querystring cxf_query_params cxf_extra_query_params}}\" />\n    {{applyUnmarshal \"xml\"}}\n    <process ref=\"processorXMLtoJSON\"/>\n    {{applyUnmarshal \"json\"}}\n    {{#if (not_empty code)}}\n        <setBody><simple trim=\"false\"><![CDATA[{{safe code}}]]></simple></setBody>\n    {{/if}}\n    <multicast>\n        {{#each target}}\n        <to uri=\"direct:{{this}}\"/>\n        {{/each}}\n    </multicast>\n</route>",
        "json_ui_schema": "{\n    \"label\": {\n        \"ui:columnSize\": \"12\"\n    },\n    \"hostname\": {\n        \"ui:columnSize\": \"4\"\n    },\n    \"port\": {\n        \"ui:columnSize\": \"3\"\n    },\n    \"path\": {\n        \"ui:columnSize\": \"5\"\n    },\n    \"soap_action\": {\n        \"ui:soap_action\": \"9\"\n    },\n    \"code\": {\n        \"ui:columnSize\": \"12\",\n        \"ui:widget\": \"AceEditorWidget\",\n        \"ui:mode\": \"javascript\",\n        \"ui:disableValidation\": true,\n        \"ui:beautify\": false,\n        \"ui:height\": 200\n    }\n}",
        "package_version": "1.0.0",
        "json_data_schema": "{\n    \"description\": \"XML to JSON\",\n    \"type\": \"object\",\n    \"required\": [\n        \"label\",\n        \"hostname\",\n        \"port\",\n        \"code\"\n    ],\n    \"properties\": {\n        \"label\": {\n            \"title\": \"Name\",\n            \"type\": \"string\"\n        },\n        \"hostname\": {\n            \"title\": \"Host\",\n            \"type\": \"string\"\n        },\n        \"port\": {\n            \"title\": \"Puerto\",\n            \"type\": \"number\"\n        },\n        \"path\": {\n            \"title\": \"Ruta\",\n            \"type\": \"string\"\n        },\n        \"soap_action\": {\n            \"title\": \"SOAP Action\",\n            \"type\": \"string\"\n        },\n        \"code\": {\n            \"title\": \"Mensaje\",\n            \"type\": \"string\"\n        },\n        \"cxf_extra_query_params\": {\n            \"title\": \"Opciones CXF\",\n            \"type\": \"array\",\n            \"items\": {\n                \"type\": \"object\",\n                \"required\": [\n                    \"code\",\n                    \"value\"\n                ],\n                \"properties\": {\n                    \"code\": {\n                        \"title\": \"Clave\",\n                        \"type\": \"string\"\n                    },\n                    \"value\": {\n                        \"title\": \"Valor\",\n                        \"type\": \"string\"\n                    }\n                }\n            }\n        }\n    }\n}",
        "react_component_type": "input"
    },
