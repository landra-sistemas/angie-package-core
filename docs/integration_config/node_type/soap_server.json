    "data": {
        "id": "e64f42c9-9628-4536-8549-ad9fe1906e0c",
        "code": "soap_server",
        "name": "SOAP Server",
        "group": "Comunicaciones",
        "name_": "SOAP Client CLONED",
        "handles": "in",
        "defaults": "{\n    \"protocol\": \"http\",\n    \"cxf_query_params\": {\n        \"dataFormat\": \"RAW\"\n    },\n    \"responses\": [\n        {\n            \"condition\": \"default\",\n            \"code\": \"\"\n        }\n    ],\n    \"input_format\": \"json\",\n    \"output_format\": \"json\"\n}",
        "package_code": "angie.core",
        "xml_template": "<route id=\"{{source}}\">\n    {{#if (not_empty soap_action)}}\n    <setHeader name=\"SOAPAction\">\n        <constant>\n            {{soap_action}}\n        </constant>\n    </setHeader>\n    {{/if}}\n    <from uri=\"cxf://{{protocol}}://{{hostname}}:{{port}}{{path}}{{querystring cxf_query_params cxf_extra_query_params}}\" />\n    {{applyUnmarshal output_format output_format_options}}\n    {{applyMarshal output_format output_format_options}}\n    <multicast>\n        {{#each target}}\n        <to uri=\"direct:{{this}}\"/>\n        {{/each}}\n    </multicast>\n    {{applyUnmarshal output_format output_format_options}}\n    <choice>\n        {{#each responses}}\n            {{#if (ne this.condition \"default\")}}\n        <when>\n            <simple>\n                {{this.condition}}\n            </simple>\n            <setBody>\n                <simple trim=\"false\">\n<![CDATA[{{safe this.code}}]]>\n                </simple>\n            </setBody>\n        </when>\n        {{/if}}\n             {{#if (eq this.condition \"default\")}}\n        <otherwise>\n            <setBody>\n                <simple trim=\"false\">\n<![CDATA[{{safe this.code}}]]>\n                </simple>\n            </setBody>\n        </otherwise>\n        {{/if}}\n        {{/each}}\n    </choice>\n</route>",
        "json_ui_schema": "{\n    \"label\": {\n        \"ui:columnSize\": \"12\"\n    },\n    \"hostname\": {\n        \"ui:columnSize\": \"4\"\n    },\n    \"port\": {\n        \"ui:columnSize\": \"3\"\n    },\n    \"path\": {\n        \"ui:columnSize\": \"5\"\n    },\n    \"soap_action\": {\n        \"ui:columnSize\": \"9\"\n    },\n    \"responses\": {\n        \"items\": {\n            \"code\": {\n                \"ui:columnSize\": \"12\",\n                \"ui:widget\": \"AceEditorWidget\",\n                \"ui:mode\": \"javascript\",\n                \"ui:disableValidation\": true,\n                \"ui:beautify\": false,\n                \"ui:height\": 200\n            }\n        }\n    }\n}",
        "package_version": "1.1.0",
        "json_data_schema": "{\n    \"description\": \"XML to JSON\",\n    \"type\": \"object\",\n    \"required\": [\n        \"label\",\n        \"hostname\",\n        \"port\",\n        \"code\"\n    ],\n    \"properties\": {\n        \"label\": {\n            \"title\": \"Name\",\n            \"type\": \"string\"\n        },\n        \"hostname\": {\n            \"title\": \"Host\",\n            \"type\": \"string\"\n        },\n        \"port\": {\n            \"title\": \"Puerto\",\n            \"type\": \"number\"\n        },\n        \"path\": {\n            \"title\": \"Ruta\",\n            \"type\": \"string\"\n        },\n        \"soap_action\": {\n            \"title\": \"SOAP Action\",\n            \"type\": \"string\"\n        },\n        \"responses\": {\n            \"title\": \"Respuestas\",\n            \"type\": \"array\",\n            \"items\": {\n                \"type\": \"object\",\n                \"properties\": {\n                    \"condition\": {\n                        \"title\": \"Condici√≥n\",\n                        \"type\": \"string\"\n                    },\n                    \"code\": {\n                        \"title\": \"Respuesta\",\n                        \"type\": \"string\"\n                    }\n                }\n            }\n        },\n        \"cxf_extra_query_params\": {\n            \"title\": \"Opciones CXF\",\n            \"type\": \"array\",\n            \"items\": {\n                \"type\": \"object\",\n                \"required\": [\n                    \"code\",\n                    \"value\"\n                ],\n                \"properties\": {\n                    \"code\": {\n                        \"title\": \"Clave\",\n                        \"type\": \"string\"\n                    },\n                    \"value\": {\n                        \"title\": \"Valor\",\n                        \"type\": \"string\"\n                    }\n                }\n            }\n        }\n    }\n}",
        "react_component_type": "AngieInputNode"
    }
